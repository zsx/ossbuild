<?xml version="1.0" encoding="UTF-8"?>

<!--
	/resources/extraction/ossbuild.extract.xml

	transient: if the resources already exists at the destination, replace it if the resource is newer.
	
	Variables:
		${home}   user's home directory
		${tmp}    temp directory
-->
<Resources xmlns="http://code.google.com/p/ossbuild/">

	<Extract package="resources.extraction" directory="${tmp}/ossbuild/">

		<Library resource="z.dll" transient="true" subDirectory="." destName="blah.dll" title="Z library" description="blah blah blah">
			<SystemAttempt>zlib.dll</SystemAttempt>
			<SystemAttempt>z.dll</SystemAttempt>
		</Library>

		<File resource="a.png" toDirectory="" toName="" title="" description="" />

	</Extract>

</Resources>
prefix=@SHARED_BUILD_DIR@
exec_prefix=${prefix}
libdir=@SHARED_BUILD_DIR@/bin
includedir=@SHARED_BUILD_DIR@/include

svgz_supported=true
css_supported=true

Name: librsvg
Description: library that renders svg files
Version: 2.26.0

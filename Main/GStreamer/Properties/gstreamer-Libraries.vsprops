<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="GStreamer Core Libraries"
	InheritedPropertySheets="$(SolutionDir)Main\GStreamer\Properties\gstreamer.vsprops"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="&quot;$(SolutionDir)\Main\GStreamer\Source\gstreamer\win32\common\&quot;"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="ws2_32.lib wsock32.lib intl.lib"
		OutputFile="$(OutDir)\bin\gstreamer\lib$(Prefix)$(ProjectName)-$(ApiMajorVersion).$(ApiMinorVersion).dll"
		Version="$(VersionString)"
		GenerateManifest="false"
		IgnoreAllDefaultLibraries="true"
		ModuleDefinitionFile=""
		ProgramDatabaseFile="$(OutDir)\pdb\gstreamer\$(TargetName).pdb"
		ImportLibrary="$(OutDir)\lib\gstreamer\$(ProjectName)-$(ApiMajorVersion).$(ApiMinorVersion).lib"
	/>
</VisualStudioPropertySheet>

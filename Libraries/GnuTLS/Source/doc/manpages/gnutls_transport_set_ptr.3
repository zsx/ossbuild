.SH SYNOPSIS
.B #include <gnutls/gnutls.h>
.sp
.BI "void gnutls_transport_set_ptr(gnutls_session_t " session ", gnutls_transport_ptr_t " ptr ");"
.SH ARGUMENTS
.IP "gnutls_session_t session" 12

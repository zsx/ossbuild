msgid "\t\t\tKey encipher only.\n"
msgstr "\t\t\tChỉ mã hoá khoá.\n"

#: lib/x509/output.c:276
msgid "\t\t\tKey decipher only.\n"
msgstr "\t\t\tChỉ giải mã khoá.\n"

